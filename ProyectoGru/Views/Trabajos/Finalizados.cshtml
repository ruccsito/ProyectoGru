
@{
    ViewBag.Title = "Finalizados";
}

@model  IEnumerable<ProyectoGru.Models.Trabajo>

<div class="row">
    <div class="col-md-12">
        <h1 class="text-center" style="color:rgb(3,134,255);font-size:39px;">Trabajos</h1>
    </div>
</div>
<div class="row" style="height:70px;"></div>
<div class="row">
    <div class="col-md-2">
        <h3 class="text-center" style="font-size:23px;">Trabajo </h3>
    </div>
    <div class="col-md-3">
        <h3 class="text-center" style="font-size:23px;">TIpo de Trabajo</h3>
    </div>
    <div class="col-md-3">
        <h3 class="text-center" style="font-size:23px;">Estado </h3>
    </div>
    <div class="col-md-4">
        <h3 class="text-center" style="font-size:23px;">Comentarios </h3>
    </div>
</div>

@foreach (var t in Model)
{
    <div class="row">
        <div class="col-md-2">
            <h4 class="text-center" style="height:17px;"> @t.ID </h4>
        </div>
        <div class="col-md-3">
            <h4 class="text-center" style="height:17px;">@t.Tipo </h4>
        </div>
        <div class="col-md-3">
            <h4>@t.Resultado</h4>
        </div>
        <div class="col-md-4">
            <h4 class="text-center" style="height:17px;">@t.Comentarios </h4>
        </div>
    </div>
}



